{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": [1, "single"],
    "eol-last": 0,
    "no-alert": 0,
    "no-debugger": 1,
    "no-mixed-requires": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-underscore-dangle": 0,
    "no-multi-spaces": 0,
    "no-trailing-spaces": 0,
    "no-extra-boolean-cast": 0,
    "no-warning-comments": 0,
    "no-unused-vars": 1,
    "new-cap": 0,
    "babel/object-shorthand": 0,
    "padded-blocks": 0,
    "comma-dangle": 1,
    "quote-props": 0,
    "max-lines": 0,
    "prefer-arrow-callback": 0,
    "camelcase": 1
  }
}
