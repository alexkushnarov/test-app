  "xo": {
    "esnext": true,
    "rules": {
      "quotes": [
        1,
        "single"
      ],
      "indent": "off",
      "no-multi-spaces": "warn",
      "babel/arrow-parens": [
        0,
        "as-needed"
      ],
      "babel/object-shorthand": "off",
      "babel/new-cap": "off",
      "padded-blocks": "off",
      "no-alert": "off",
      "comma-dangle": "warn",
      "no-trailing-spaces": "off",
      "import/no-mutable-exports": "warn",
      "quote-props": "off",
      "no-warning-comments": "off",
      "max-lines": "off",
      "no-mixed-spaces-and-tabs": "off",
      "no-unused-vars": "warn",
      "import/no-extraneous-dependencies": "off",
      "prefer-arrow-callback": "off",
      "camelcase": "warn",
      "spaced-comment": "off",
      "one-var": "warn",
      "no-var": "warn",
      "space-before-function-paren": "warn",
      "no-implicit-coercion": "off",
      "max-params": [
        "warn",
        10
      ],
      "no-negated-condition": "off",
      "no-lonely-if": "off",
      "no-extra-boolean-cast": "off",
      "curly": "warn",
      "key-spacing": "warn"
    },
    "envs": [
      "node",
      "browser",
      "es6",
      "jquery",
      "protractor",
      "jasmine"
    ]
  }
